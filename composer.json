{
    "name": "symfony/framework-standard-edition",
    "description": "The \"Symfony Standard Edition\" distribution",
    "license": "MIT",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "1.9.1",
                "dist": {
                    "url": "http://code.jquery.com/jquery-1.9.1.js",
                    "type": "file"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.5.*",
        "doctrine/orm": "2.4.*",
        "doctrine/doctrine-bundle": "1.3.*",
        "twig/extensions": "1.*",
        "symfony/assetic-bundle": "2.3.*",
        "symfony/swiftmailer-bundle": "2.3.*",
        "symfony/monolog-bundle": "2.*",
        "sensio/distribution-bundle": "3.*",
        "sensio/framework-extra-bundle": "3.*",
        "sensio/generator-bundle": "2.3.*",
        "incenteev/composer-parameter-handler": "~2.0",
        "jms/security-extra-bundle": "1.5.*",
        "jms/di-extra-bundle": "1.4.*",
        "gedmo/doctrine-extensions": "2.3.*@dev",
        "fkr/simplepie-bundle": "1.*",
        "predis/predis": "0.8.*",
        "snc/redis-bundle": "1.1.*",
        "leafo/lessphp": "0.4.*",
        "mopa/bootstrap-bundle": "2.3.*",
        "twbs/bootstrap": "2.3.2",
        "jquery/jquery": "1.*",
        "undf/angularjsbundle" :"dev-master#3628ecf",
        "ezyang/htmlpurifier": "4.6.*",
        "symfony/dom-crawler": "2.*",
        "fabpot/goutte": "2.*",
        "symfony/process": "2.*",
        "mattsches/version-eye-bundle": "dev-master",
        "mmoreram/rsqueue-bundle": "dev-master",
        "friendsofsymfony/elastica-bundle": "3.0.*@dev",
        "eko/googletranslatebundle": "dev-master",
        "endroid/gcm-bundle": "dev-master",
        "celd/opml-import-export": "dev-master",
        "duellsy/pockpack": "2.*"
    },
    //TODO: before update review "duellsy/pockpack" merged my pull request
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrap"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrap"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "branch-alias": {
            "dev-master": "2.3-dev"
        }
    }
}
