{% extends 'NPSFrontendBundle::Default/layout.html.twig' %}
{% block title %}Signup title!{% endblock title %}

{% block content %}
<div id="content">
    <div class="container_16">
        <div class="grid_16">
            <div class="pad10">
                <h1>Signup page</h1>
            </div>
        </div>
        <div class="grid_16">
            <div class="pad10">
                <form action="" method="post" {{ form_enctype(form) }}>
                    {{ form_widget(form) }}
                    <input type="submit" />
                </form>
            </div>
        </div>
    </div>
</div>
{% endblock content %}