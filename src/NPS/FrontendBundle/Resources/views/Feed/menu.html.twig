<ul id="mSubscriptionList" class="nav nav-list">
    {% for userFeed in userFeeds %}
        <li>
            <a href="{{ url("items_list", {'user_feed_id':userFeed.id}) }}">
                <span class="badge">{{ userFeed.total }}</span>
                {% if (userFeed.title | length) > 17 %}
                    {{ userFeed.title | slice(0, 17) }}...
                {% else %}
                    {{ userFeed.title }}
                {% endif %}
            </a>
        </li>
    {% endfor %}
</ul>